{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Information"
      }
    }
  },
  "Storage": {
    "Invariant": "MySql.Data.MySqlClient",
    "ConnectionString": "Server=DB_HOST_NAME;Port=25060;Database=defaultdb;Userid=XXX;Password=XXX;SslMode=Required;"
  },

  "Redis": {
    "ConnectionString": "redis:25061;user=default;ssl=true;defaultDatabase=1;password=XXX@"
  }
}
