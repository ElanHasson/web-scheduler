{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Information"
      }
    }
  },
  "Storage": {
    "Invariant": "MySql.Data.MySqlConnector",
    "ConnectionString": "Server=DB_HOST_NAME;Port=25060;Database=defaultdb;Userid=XXX;Password=XXX;SslMode=Required;"
  }
}
